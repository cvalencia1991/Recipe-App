<div id="<%= dom_id recipe_food %>">
  <p class="mb-10 mt-2 text-xl">
    <strong>Quantity:</strong>
    <%= recipe_food.quantity %>
  </p>

  <p class="w-40 flex justify-between text-lg mt-4 border-2 p-2 rounded">
    <strong>Recipe:</strong>
    <%= recipe_food.recipe_id %>
  </p>

  <p class="w-40 flex justify-between text-lg mt-4 border-2 p-2 rounded">
    <strong>Food:</strong>
    <%= recipe_food.food_id %>
  </p>

</div>
